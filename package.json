{
	"name": "mandolin-rip",
	"version": "1.0.0",
	"private": true,
	"bugs": {
		"url": "https://github.com/jonahsnider/mandolin-rip/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonahsnider/mandolin-rip.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonahsnider.com",
		"url": "https://jonahsnider.com"
	},
	"main": "./tsc_output/index.js",
	"scripts": {
		"prebuild": "rm -rf tsc_output",
		"build": "tsc",
		"lint": "xo",
		"start": "node .",
		"stitch": "node tsc_output/stitch",
		"style": "prettier --check ."
	},
	"xo": {
		"prettier": true,
		"rules": {
			"prettier/prettier": "off",
			"unicorn/no-process-exit": "off",
			"@typescript-eslint/no-implicit-any-catch": "off",
			"unicorn/prefer-module": "off"
		}
	},
	"dependencies": {
		"@jonahsnider/util": "8.1.1",
		"consola": "2.15.3",
		"delay": "5.0.0",
		"execa": "5.1.1",
		"got": "11.8.2",
		"m3u8-parser": "4.7.0",
		"p-retry": "4.6.1",
		"type-fest": "2.3.4"
	},
	"devDependencies": {
		"@tsconfig/node16": "1.0.2",
		"@types/node": "16.10.1",
		"prettier": "2.3.2",
		"prettier-config-xo": "2.0.0",
		"ts-node": "10.2.1",
		"typescript": "4.3.5",
		"xo": "0.44.0"
	},
	"packageManager": "yarn@3.0.2"
}
